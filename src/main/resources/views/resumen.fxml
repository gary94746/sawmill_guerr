<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTreeTableView?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vBoxResumen" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="514.0" prefWidth="839.0" stylesheets="@../estilos/dashboard.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ResumenController">
   <children>
      <Label prefHeight="16.0" prefWidth="599.0" text="Resumen">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
         <styleClass>
            <String fx:value="labelTitle" />
            <String fx:value="labelResumen" />
         </styleClass>
      </Label>
      <JFXTreeTableView prefHeight="220.0" prefWidth="591.0" VBox.vgrow="SOMETIMES">
         <columns>
            <TreeTableColumn prefWidth="50.0" text="MEDIDA" />
            <TreeTableColumn prefWidth="75.0" text="PRIMERA" />
            <TreeTableColumn prefWidth="75.0" text="SEGUNDA" />
            <TreeTableColumn text="TERCERA BUENA" />
            <TreeTableColumn prefWidth="75.0" text="TERCERA MALA" />
            <TreeTableColumn prefWidth="85.0" text="MADERA CRUZADA" />
            <TreeTableColumn prefWidth="75.0" text="CUADRADO" />
            <TreeTableColumn prefWidth="75.0" text="VIGA" />
            <TreeTableColumn prefWidth="75.0" text="TOTAL" />
         </columns>
         <columnResizePolicy>
            <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </JFXTreeTableView>
      <GridPane hgap="20.0" vgap="5.0">
        <columnConstraints>
          <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints fillWidth="false" halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Volumen Aserrado:" GridPane.rowIndex="1">
               <styleClass>
                  <String fx:value="labelResumen" />
                  <String fx:value="labelResult" />
               </styleClass></Label>
            <Label text="Coeficiente de Aserrio:" GridPane.rowIndex="2">
               <styleClass>
                  <String fx:value="labelResumen" />
                  <String fx:value="labelResult" />
               </styleClass></Label>
            <Label text="Volumen en Rollo:">
               <styleClass>
                  <String fx:value="labelResumen" />
                  <String fx:value="labelResult" />
               </styleClass></Label>
            <TextField editable="false" styleClass="textField" GridPane.columnIndex="1" />
            <TextField editable="false" styleClass="textField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField editable="false" styleClass="textField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </GridPane>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
