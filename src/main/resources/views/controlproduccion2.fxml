               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <JFXButton contentDisplay="CENTER" style="-fx-background-color: #916b47;" text="." textFill="WHITE">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
               <graphic>
                  <FontAwesomeIconView fill="WHITE" glyphName="PLUS" />
               </graphic>
            </JFXButton>
            <JFXButton contentDisplay="CENTER" layoutX="538.0" layoutY="29.0" style="-fx-background-color: #916b47;" text="." textFill="WHITE">
               <graphic>
                  <FontAwesomeIconView fill="WHITE" glyphName="EXTERNAL_LINK_SQUARE" />
               </graphic>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </JFXButton>
         </children>
      </HBox>
      <JFXTreeTableView prefHeight="233.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
