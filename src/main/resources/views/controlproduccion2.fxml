            <TreeTableColumn prefWidth="75.0" text="Cubicacion" />
            <TreeTableColumn prefWidth="75.0" text="Pies-tabla" />
         </columns>
         <columnResizePolicy>
            <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </JFXTreeTableView>
      <HBox alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <children>
            <Label contentDisplay="RIGHT" text="Sub-total:">
               <HBox.margin>
                  <Insets right="2.0" top="10.0" />
               </HBox.margin>
            </Label>
            <JFXTextField prefHeight="25.0" prefWidth="87.0" />
