         <font>
            <Font name="Open Sans Condensed Bold" size="22.0" />
         </font>
      </Label>
      <Label text="Tabletas.">
         <VBox.margin>
            <Insets left="20.0" top="10.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER" prefHeight="62.0" prefWidth="600.0">
         <children>
            <Label contentDisplay="RIGHT" text="Longitud:">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <graphic>
                  <JFXComboBox />
               </graphic>
            </Label>
            <Label contentDisplay="RIGHT" layoutX="10.0" layoutY="10.0" text="Grueso x Ancho:">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
               <graphic>
                  <JFXComboBox prefHeight="25.0" prefWidth="93.0" />
               </graphic>
            </Label>
            <Label contentDisplay="RIGHT" layoutX="177.0" layoutY="10.0" text="Piezas:">
               <graphic>
                  <JFXTextField prefHeight="25.0" prefWidth="57.0" />
